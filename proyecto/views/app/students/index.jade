extends ../../layout.jade
block contenido
    div(class="container")
        h1 Mis Imagenes
        table(class="table table-bordered")
            thead
                tr
                    th Nombre
                    th Apellido
                    th Direccion
                    th Telefono
                    th Fecha Nacimiento
                    th Acciones
            tbody
                for student in students
                    tr
                        td=student.name
                        td=student.last_name
                        td=student.address
                        td=student.phone
                        td=student.birthdate
                        td 
                            a(href="/app/students/new", class="btn btn-info right-space") Editar

                            a(href="/app/students/new", class="btn btn-danger") Eliminar
        br
        a(href="/app/students/new", class="btn btn-primary") Nuevo Estudiante
