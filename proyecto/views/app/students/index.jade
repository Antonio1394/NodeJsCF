extends ../../layout.jade
block contenido
    div(class="container")
        h1 Mis Imagenes
        table(class="table table-bordered")
            thead
                tr
                    th Nombre
                    th Apellido
                    th Direccion
            tbody
                for student in students
                    tr
                        td=student.name
                        td=student.last_name
                        td=student.address
                       
        br
        a(href="/app/students/new", class="btn btn-primary") Nuevo Estudiante
